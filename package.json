{
  "name": "@pascaliske/ngx-prerenderer",
  "description": "Angular Universal Prerenderer.",
  "version": "1.0.1",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-prerenderer",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-prerenderer/issues"
  },
  "main": "dist/index.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "watch": "nodemon --exec \"rm -rf dist typings && yarn run build\" --watch src --ext ts",
    "lint": "tslint --project tsconfig.json --fix",
    "check": "snyk test && snyk monitor",
    "format": "prettier --write \"src/**/*.ts\"",
    "changelog": "standard-changelog --release-count 1",
    "link": "yarn run build && yarn link",
    "unlink": "yarn unlink"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "peerDependencies": {
    "@angular/common": "^6.0.0 || ^7.0.0",
    "@angular/core": "^6.0.0 || ^7.0.0",
    "@angular/router": "^6.0.0 || ^7.0.0",
    "chalk": "^2.0.0",
    "fs-extra": "^7.0.1"
  },
  "devDependencies": {
    "@angular/common": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/router": "^7.2.3",
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.21",
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.3",
    "snyk": "^1.124.0",
    "standard-changelog": "^2.0.6",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.3.1"
  }
}

{
  "name": "@pascaliske/ngx-prerenderer",
  "description": "Angular Universal Prerenderer.",
  "version": "1.1.0",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-prerenderer",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-prerenderer/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts",
    "watch": "rollup --config rollup.config.ts --watch",
    "postbuild": "tsc --declaration --outDir dist --emitDeclarationOnly src/index.ts",
    "lint": "tslint --project tsconfig.json --fix",
    "check": "snyk test && snyk monitor",
    "format": "prettier --write \"src/**/*.ts\"",
    "changelog": "standard-changelog --release-count 1",
    "link": "yarn run build && yarn link",
    "unlink": "yarn unlink"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "peerDependencies": {
    "@angular/common": "^6.0.0 || ^7.0.0",
    "@angular/core": "^6.0.0 || ^7.0.0",
    "@angular/router": "^6.0.0 || ^7.0.0",
    "chalk": "^2.0.0",
    "fs-extra": "^7.0.1"
  },
  "devDependencies": {
    "@angular/common": "^7.2.12",
    "@angular/core": "^7.2.12",
    "@angular/router": "^7.2.12",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/fs-extra": "^5.0.5",
    "@types/node": "^11.13.0",
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "rollup": "^1.10.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-execute": "^1.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.20.1",
    "snyk": "^1.147.3",
    "standard-changelog": "^2.0.7",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.1"
  }
}

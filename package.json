{
  "name": "@pascaliske/ngx-prerenderer",
  "description": "Angular Universal Prerenderer.",
  "version": "2.0.4",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-prerenderer",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-prerenderer/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts",
    "watch": "rollup --config rollup.config.ts --watch",
    "lint": "tslint --project tsconfig.json --fix",
    "check": "snyk test && snyk monitor",
    "format": "prettier --write \"src/**/*.ts\"",
    "version": "bump --yes package.json",
    "changelog": "standard-changelog",
    "link": "yarn run build && yarn link",
    "unlink": "yarn unlink"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@pascaliske/prettier-config",
  "dependencies": {
    "mkdirp": "^1.0.4",
    "tslib": "^1.11.1"
  },
  "peerDependencies": {
    "@angular/common": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/core": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/platform-server": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/router": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0 || ^7.0.0"
  },
  "optionalDependencies": {
    "chalk": "^4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^9.1.2",
    "@angular/core": "^9.1.2",
    "@angular/router": "^9.1.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@pascaliske/prettier-config": "^1.0.2",
    "@pascaliske/tsconfig": "^1.0.4",
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^13.13.1",
    "bump-cli": "^1.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "standard-changelog": "^2.0.21",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}

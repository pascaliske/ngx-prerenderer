{
  "name": "@pascaliske/ngx-prerenderer",
  "description": "Angular Universal Prerenderer.",
  "version": "2.0.3",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-prerenderer",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-prerenderer/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts",
    "watch": "rollup --config rollup.config.ts --watch",
    "lint": "tslint --project tsconfig.json --fix",
    "check": "snyk test && snyk monitor",
    "format": "prettier --write \"src/**/*.ts\"",
    "changelog": "standard-changelog --release-count 1",
    "link": "yarn run build && yarn link",
    "unlink": "yarn unlink"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "tslib": "^1.10.0"
  },
  "peerDependencies": {
    "@angular/common": "^7.0.0 || ^8.0.0",
    "@angular/core": "^7.0.0 || ^8.0.0",
    "@angular/platform-server": "^7.0.0 || ^8.0.0",
    "@angular/router": "^7.0.0 || ^8.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0 || ^7.0.0"
  },
  "optionalDependencies": {
    "chalk": "^3.0.0"
  },
  "devDependencies": {
    "@angular/common": "^8.2.13",
    "@angular/core": "^8.2.13",
    "@angular/router": "^8.2.13",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@pascaliske/tsconfig": "^1.0.4",
    "@types/fs-extra": "^8.0.1",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^13.1.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rollup": "^1.27.14",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "snyk": "^1.268.0",
    "standard-changelog": "^2.0.21",
    "tslint": "^6.0.0-beta1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  }
}
